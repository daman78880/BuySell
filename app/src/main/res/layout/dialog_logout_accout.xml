<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/transparent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardUseCompatPadding="true"
        app:cardMaxElevation="@dimen/_18sdp"
        app:cardCornerRadius="@dimen/_8sdp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        >
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/txtTitleLogoutDialog"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/logout"
            android:fontFamily="@font/inter_medium"
            android:textColor="@color/signUpBtnColor"
            android:textSize="@dimen/_14sdp"
            android:layout_marginTop="@dimen/topMainMargin"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            />
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/txtSubTitleLogoutDialog"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/logoutAccountMessage"
            android:fontFamily="@font/inter"
            android:textColor="@color/greyColorForDialog"
            android:textSize="@dimen/normalTextSize"
            android:gravity="center"
            android:layout_marginStart="@dimen/_40sdp"
            android:layout_marginEnd="@dimen/_40sdp"
            android:layout_marginTop="@dimen/_5sdp"
            app:layout_constraintTop_toBottomOf="@+id/txtTitleLogoutDialog"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            />
        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnNotNowLogoutDialog"
            android:layout_width="@dimen/_95sdp"
            android:layout_height="@dimen/_35sdp"
            android:text="@string/not_now"
            android:textAllCaps="false"
            android:textSize="@dimen/normalTextSize"
            android:fontFamily="@font/inter_medium"
            android:background="@drawable/edit_text_round"
            android:layout_marginTop="@dimen/topMainMargin"
            android:layout_marginBottom="@dimen/topMainMargin"
            app:layout_constraintTop_toBottomOf="@+id/txtSubTitleLogoutDialog"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/btnLogoutDialog"
            app:layout_constraintBottom_toBottomOf="parent"
            />
        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnLogoutDialog"
            android:layout_width="@dimen/_95sdp"
            android:layout_height="@dimen/_35sdp"
            android:text="@string/logout"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textSize="@dimen/normalTextSize"
            android:fontFamily="@font/inter_medium"
            android:background="@drawable/round_sign_up_btn"
            app:layout_constraintTop_toTopOf="@+id/btnNotNowLogoutDialog"
            app:layout_constraintBottom_toBottomOf="@+id/btnNotNowLogoutDialog"
            app:layout_constraintStart_toEndOf="@+id/btnNotNowLogoutDialog"
            app:layout_constraintEnd_toEndOf="parent"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>