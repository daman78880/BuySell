<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".screens.guestaccount.GuestAccountFragment">
    <include
        android:id="@+id/appBarGuestAccount"
        layout="@layout/toolbar_layout_one"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/appBarGuestAccount"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        >
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <ImageView
                android:id="@+id/imgUserImageGuestAccount"
                android:layout_width="@dimen/_90sdp"
                android:layout_height="@dimen/_90sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:background="@drawable/user_image_round"
                android:clipToOutline="true"
                android:scaleType="centerCrop"
                android:contentDescription="@string/description"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:targetApi="s" />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/txtUserNameGuestAccount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8sdp"
                android:fontFamily="@font/inter_semibold"
                android:text="@string/welcome_to_buy_amp_sell"
                android:textColor="@color/signUpBtnColor"
                android:textSize="@dimen/_15sdp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imgUserImageGuestAccount" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/txtSubTextGuestAccount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_3sdp"
                android:fontFamily="@font/inter"
                android:text="@string/you_need_to_login_or_register"
                android:textColor="@color/signUpBtnColor"
                android:textSize="@dimen/_11sdp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtUserNameGuestAccount" />
         <View
                android:id="@+id/viewLineHelpAndSupportGuestAccount"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/viewLine"
                android:layout_marginStart="@dimen/_6sdp"
                android:layout_marginEnd="@dimen/_6sdp"
                android:layout_marginTop="@dimen/_10sdp"
                app:layout_constraintTop_toBottomOf="@+id/txtSubTextGuestAccount"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                />
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cLayoutHelpAndSupportGuestAccount"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_7sdp"
                android:layout_marginEnd="@dimen/_7sdp"
                android:padding="@dimen/_10sdp"
                app:layout_constraintTop_toBottomOf="@+id/viewLineHelpAndSupportGuestAccount"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                >
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/imgHelpAndSupportIconGuestAccount"
                    android:layout_width="wrap_content"
                    android:layout_height="22dp"
                    android:src="@drawable/ic_baseline_wifi_calling_3_24"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/txtHelpAndSupportProfileGuestAccount"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/txtHelpAndSupportProfileGuestAccount"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/help_amp_support"
                    android:fontFamily="@font/inter"
                    android:textColor="@color/signUpBtnColor"
                    android:textSize="@dimen/_11sdp"
                    android:paddingStart="@dimen/_12sdp"
                    app:layout_constraintStart_toEndOf="@+id/imgHelpAndSupportIconGuestAccount"
                    app:layout_constraintEnd_toStartOf="@+id/imgRightHelpAndSupportArrowGuestAccount"
                    app:layout_constraintTop_toTopOf="@+id/imgHelpAndSupportIconGuestAccount"
                    app:layout_constraintBottom_toBottomOf="@+id/imgHelpAndSupportIconGuestAccount"
                    tools:ignore="RtlSymmetry" />
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/imgRightHelpAndSupportArrowGuestAccount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_baseline_keyboard_arrow_right_24"
                    app:layout_constraintTop_toTopOf="@+id/txtHelpAndSupportProfileGuestAccount"
                    app:layout_constraintBottom_toBottomOf="@+id/txtHelpAndSupportProfileGuestAccount"
                    app:layout_constraintStart_toEndOf="@+id/txtHelpAndSupportProfileGuestAccount"
                    app:layout_constraintEnd_toEndOf="parent"
                    />
            </androidx.constraintlayout.widget.ConstraintLayout>
            <View
                android:id="@+id/viewLineLoginRegisterGuestAccount"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/viewLine"
                android:layout_marginStart="@dimen/_6sdp"
                android:layout_marginEnd="@dimen/_6sdp"
                app:layout_constraintTop_toBottomOf="@+id/cLayoutHelpAndSupportGuestAccount"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                />
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cLayoutLoginRegisterGuestAccount"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_7sdp"
                android:layout_marginEnd="@dimen/_7sdp"
                android:padding="@dimen/_10sdp"
                app:layout_constraintTop_toBottomOf="@+id/viewLineLoginRegisterGuestAccount"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                >
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/imgLoginRegisterIconGuestAccount"
                    android:layout_width="wrap_content"
                    android:layout_height="22dp"
                    android:src="@drawable/ic_baseline_person_24"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/txtLoginRegisterProfileGuestAccount"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/txtLoginRegisterProfileGuestAccount"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/login_or_register"
                    android:fontFamily="@font/inter"
                    android:textColor="@color/signUpBtnColor"
                    android:textSize="@dimen/_11sdp"
                    android:paddingStart="@dimen/_12sdp"
                    app:layout_constraintStart_toEndOf="@+id/imgLoginRegisterIconGuestAccount"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/imgLoginRegisterIconGuestAccount"
                    app:layout_constraintBottom_toBottomOf="@+id/imgLoginRegisterIconGuestAccount"
                    tools:ignore="RtlSymmetry" />

            </androidx.constraintlayout.widget.ConstraintLayout>
            <View
                android:id="@+id/viewLineLoginRegisterEndGuestAccount"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/viewLine"
                android:layout_marginStart="@dimen/_6sdp"
                android:layout_marginEnd="@dimen/_6sdp"
                app:layout_constraintTop_toBottomOf="@+id/cLayoutLoginRegisterGuestAccount"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>