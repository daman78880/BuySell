<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txtTempCountryCodeDialog"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/select_country"
        android:layout_marginStart="@dimen/normalStartMagin"
        android:layout_marginTop="@dimen/topMainMargin"
        android:fontFamily="@font/inter_bold"
        android:textAllCaps="false"
        android:textColor="@color/signUpBtnColor"
        android:textSize="@dimen/liteBigTextSize"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/txtTempCancelCountryCodeDialog"
        />
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txtTempCancelCountryCodeDialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cancel"
      android:layout_marginEnd="@dimen/normalStartMagin"
        android:layout_marginTop="@dimen/topMainMargin"
        android:fontFamily="@font/inter_bold"
        android:textAllCaps="false"
        android:textColor="@color/blue_Txt"
        android:textSize="@dimen/liteBigTextSize"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtTempCountryCodeDialog"
        app:layout_constraintEnd_toEndOf="parent"
        />
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cLSearchCountryCodeDialog"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/et_filter"
        android:layout_marginTop="@dimen/topMainMargin"
        android:layout_marginStart="@dimen/lowStartMagin"
        android:layout_marginEnd="@dimen/lowStartMagin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtTempCountryCodeDialog"
        >
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/imgTempCountryCodeDialog"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_7sdp"
            android:src="@drawable/ic_baseline_search_24"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            />
        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/etNameCountryCodeDialog"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:padding="@dimen/_9sdp"
            android:hint="@string/search"
            android:singleLine="true"
            android:textSize="@dimen/normalTextSize"
            android:textColor="@color/liteGreay"
            android:fontFamily="@font/inter_medium"
            android:background="@android:color/transparent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imgTempCountryCodeDialog"
            app:layout_constraintEnd_toEndOf="parent"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvcountryDialog"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:orientation="vertical"
        android:background="@color/white"
        tools:listitem="@layout/rv_namelist_country_code_dialog"
        android:layout_marginTop="@dimen/topMargin"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
        app:layout_constraintTop_toBottomOf="@+id/cLSearchCountryCodeDialog"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        />


</androidx.constraintlayout.widget.ConstraintLayout>